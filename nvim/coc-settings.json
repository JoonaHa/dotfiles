  //  "enum": "\uf435",
  //  "enumMember": "\uf02b",
  //  "module": "\uf40d",
  //  "color": "\ue22b",
  //  "property": "\ue624",
  //  "field": "\uf9be",
  //  "unit": "\uf475",
  //  "event": "\ufacd",
